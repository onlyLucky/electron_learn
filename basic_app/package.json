{
  "name": "basic-app",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "",
  "license": "",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "dist": "electron-builder --win --x64"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "build": {
    "appId": "com.basic.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "files": [
      "dist/**/*",
      "main.js",
      "dist-electron/**/*",
      "electron/**/*"
    ],
    "extends": null
  },
  "dependencies": {
    "view-ui-plus": "^1.3.1",
    "vue-i18n": "^9.2.2",
    "vue": "^3.2.40",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "babel-plugin-import": "^1.13.5",
    "electron": "^22.0.0",
    "electron-builder": "^23.3.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vue-tsc": "^1.0.9"
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
